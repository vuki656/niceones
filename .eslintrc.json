{
    "globals": {
        "__PATH_PREFIX__": true
    },
    "overrides": [
        {
            "files": ["**/*.ts", "**/*.tsx"],
            "plugins": ["@typescript-eslint"]
        }
    ],
    "extends": [
        "@dvukovic/eslint-config/core",
        "@dvukovic/eslint-config/react"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.json"
    }
}
